    "            else:\n",
    "                delays=nx.dijkstra_path_length(graph_isl,sd_now[0],gsl[safe_gs])\n",
    "                delayd=nx.dijkstra_path_length(graph_isl,gsl[safe_gs],sd_now[1])\n",
    "                sg_dis=2*get_arc([satPos[gsl[safe_gs]]['lng'],satPos[gsl[safe_gs]]['lat']],[gateway[safe_gs]['lng'],gateway[safe_gs]['lat']])/LIGHTSPEED\n",
    "                rtt_now=sg_dis+delays+delayd\n",
