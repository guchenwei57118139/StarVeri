    "def analysis_alibi(window,sd_pre0,gsl_init0,rtt_init0,sd_pre1,rtt_init1,gsl_init1,safe_gs,risk_area,start,end,sd_no,risk_country,ineq_factor,gateway,result_now):\n",
    "    '''\n",
    "    rtt_init:dict：[sd_no][gid_relay]=time0, [gid][sd_no]=time1--src and gid are sd no, not satID\n",
    "    '''\n",
    "    def cal_risk_sats(risk_area,satPos):\n",
    "        risk_sats=[]\n",
    "        for sid in range(1584):\n",
    "            if risk_area.contains(Point(satPos[sid]['lng'],satPos[sid]['lat'])):\n",
    "                risk_sats.append(sid)\n",
    "        return risk_sats\n",
    "    def if_risk_sats(path,risk_sats):\n",
    "        for node in path[1:]:\n",
    "            if node in risk_sats:\n",
    "                return 1\n",
    "        return 0\n",
    "#     def writef(file,t,FP,FN,unreach,abnormal):\n",
