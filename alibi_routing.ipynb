    "def load_topo(constell_id,sat):\n",
    "    GRAPH_ISL=nx.Graph()\n",
    "    constell=constell_id\n",
    "    i=0\n",
    "    for orbitId in range(NUM_ORBITS[constell]):\n",
    "        for satInOrbitId in range(NUM_SATS_PER_ORBIT[constell]):\n",
    "            satId = orbitId * NUM_SATS_PER_ORBIT[constell] + satInOrbitId + satStartId[i] # satID = pre sat + inner no\n",
    "            #GRAPH_ISL.add_node(satId)\n",
    "            neighborOrbitId = orbitId\n",
    "            neighborSatInOrbitId = (satInOrbitId + 1) % NUM_SATS_PER_ORBIT[constell]\n",
    "            neighborSatId = neighborOrbitId * NUM_SATS_PER_ORBIT[constell] + neighborSatInOrbitId + satStartId[i]\n",
    "            dis=get_arc([sat[satId]['lng'],sat[satId]['lat']],[sat[neighborSatId]['lng'],sat[neighborSatId]['lat']])\n",
    "            GRAPH_ISL.add_edge(satId, neighborSatId,weight=dis/LIGHTSPEED)\n",
    "            # down\n",
    "            neighborOrbitId = orbitId\n",
    "            neighborSatInOrbitId = (satInOrbitId - 1 + NUM_SATS_PER_ORBIT[constell]) % NUM_SATS_PER_ORBIT[constell]\n",
    "            neighborSatId = neighborOrbitId * NUM_SATS_PER_ORBIT[constell] + neighborSatInOrbitId + satStartId[i]\n",
    "            dis=get_arc([sat[satId]['lng'],sat[satId]['lat']],[sat[neighborSatId]['lng'],sat[neighborSatId]['lat']])\n",
    "            GRAPH_ISL.add_edge(satId, neighborSatId,weight=dis/LIGHTSPEED)\n",
    "            # left\n",
    "            neighborOrbitId = (orbitId - 1 + NUM_ORBITS[constell]) % NUM_ORBITS[constell]\n",
    "            neighborSatInOrbitId = satInOrbitId\n",
    "            neighborSatId = neighborOrbitId * NUM_SATS_PER_ORBIT[constell] + neighborSatInOrbitId + satStartId[i]\n",
    "            dis=get_arc([sat[satId]['lng'],sat[satId]['lat']],[sat[neighborSatId]['lng'],sat[neighborSatId]['lat']])\n",
    "            GRAPH_ISL.add_edge(satId, neighborSatId,weight=dis/LIGHTSPEED)\n",
    "            # right\n",
    "            neighborOrbitId = (orbitId + 1) % NUM_ORBITS[constell]\n",
    "            neighborSatInOrbitId = satInOrbitId\n",
    "            neighborSatId = neighborOrbitId * NUM_SATS_PER_ORBIT[constell] + neighborSatInOrbitId + satStartId[i]\n",
    "            dis=get_arc([sat[satId]['lng'],sat[satId]['lat']],[sat[neighborSatId]['lng'],sat[neighborSatId]['lat']])\n",
    "            GRAPH_ISL.add_edge(satId, neighborSatId,weight=dis/LIGHTSPEED)\n",
    "    return GRAPH_ISL\n",
