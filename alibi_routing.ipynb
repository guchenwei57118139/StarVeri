    "#         fw.write(f'{str(t)}\\n{str((FP,FN,unreach,abnormal))}\\n')\n",
    "#         fw.close()\n",
    "    flag=0\n",
    "    sd_pre,gsl_init,rtt_init=sd_pre0,gsl_init0,rtt_init0\n",
    "    #with tqdm(range(start,end)) as zone:\n",
    "    for t in tqdm(range(start,end)):\n",
    "#             fn=f'/mnt/data/research3/tmp_chenwei_disk/path_validation/change_interval/result_shortest_gs_alibi/{risk_country}/{ineq_factor}/{sd_no}/{t}.txt'\n",
    "        fn=f'/data/chenwei/research3/data/result_shortest_gs_alibi/{risk_country}/{ineq_factor}/{sd_no}/{t}.txt'\n",
    "\n",
    "        change_flag=0\n",
